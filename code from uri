#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    float avg,sum=0;
    int count=0;
     while(!0)
     {
         int age;
         cin>>age;
         if(age<0)
         {
             break;
         }
        sum = sum+age;
        count++;
     }
     avg=(sum)/count;
     cout<<fixed<<setprecision(2)<<avg<<endl;
}
